{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_display_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_authors}"
	],
	"contact": {
		"homepage": "${mod_homepage}",
		"issues": "${mod_issues_tracker}",
		"sources": "${mod_git_repo}"
	},
	"license": "${mod_license}",
	"icon": "${mod_id}.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.github.myname.mymod.MyMod"
		],
		"client": [
			"com.github.myname.mymod.client.MyModClient"
		]
	},
	"mixins": [
		"${mod_id}.mixins.json"
	],
	"accessWidener": "${mod_id}.accesswidener",
	"depends": {
   		"fabricloader": ">=${fabric_loader_version_range}",
		"fabric-api": ">=${fabric_api_version_range}",
		"java": ">=${java_version}",
		"minecraft": ">=${minecraft_version}"
	}
}